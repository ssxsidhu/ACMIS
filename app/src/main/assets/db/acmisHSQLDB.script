CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE USERS(USERID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,FIRSTNAME VARCHAR(20) NOT NULL,LASTNAME VARCHAR(20) NOT NULL,GENDER VARCHAR(6),USERNAME VARCHAR(20) NOT NULL,EMAIL VARCHAR(20),PHONENUMBER INTEGER)
CREATE MEMORY TABLE LOCATIONS(LOCATIONID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,CITY VARCHAR(20) NOT NULL,COUNTRY VARCHAR(20),AIRPORT VARCHAR(6),TIMEZONE VARCHAR(40))
CREATE MEMORY TABLE BOOKINGS(BOOKINGID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,USERID INTEGER,ROUTEDEPART VARCHAR(100), ROUTERETURN VARCHAR(100), NUMPASSENGERS INTEGER)
CREATE MEMORY TABLE FLIGHTS(FLIGHTID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,STARTLOCATION INTEGER,ENDLOCATION INTEGER,YEAR INTEGER,MONTH INTEGER,DAY INTEGER,HOUR INTEGER,MINUTE INTEGER,SEATS INTEGER,DURATION DOUBLE,COST INTEGER)
ALTER TABLE USERS ALTER COLUMN USERID RESTART WITH 0
ALTER TABLE LOCATIONS ALTER COLUMN LOCATIONID RESTART WITH 0
ALTER TABLE BOOKINGS ALTER COLUMN BOOKINGID RESTART WITH 1
ALTER TABLE FLIGHTS ALTER COLUMN FLIGHTID RESTART WITH 1
