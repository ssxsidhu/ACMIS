<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <item
        android:width="100dp"
        android:height="10dp"
        android:gravity="center">
        <rotate android:fromDegrees="180">
            <shape android:shape="line">
                <stroke
                    android:width="1dp"
                    android:color="@color/md_theme_dark_onSurfaceVariant" />
            </shape>
        </rotate>
    </item>
    <item
        android:width="8dp"
        android:height="8dp"
        android:gravity="start|center">
        <shape android:shape="oval">
            <stroke
                android:width="0.7dp"
                android:color="@color/md_theme_dark_onSurfaceVariant" />
            <solid android:color="@color/design_default_color_background" />
        </shape>
    </item>

    <item
        android:width="8dp"
        android:height="8dp"
        android:gravity="end|center">
        <shape android:shape="oval">
            <stroke
                android:width="0.7dp"
                android:color="@color/md_theme_dark_onSurfaceVariant" />
            <solid android:color="@color/design_default_color_background" />
        </shape>
    </item>

    <item
        android:width="8dp"
        android:height="8dp"
        android:left="30dp"
        android:gravity="start|center">
        <shape android:shape="oval">
            <stroke
                android:width="0.7dp"
                android:color="@color/md_theme_dark_shadow" />
            <solid android:color="@color/md_theme_dark_shadow" />
        </shape>
    </item>

    <item
        android:width="8dp"
        android:height="8dp"
        android:right="30dp"
        android:gravity="end|center">
        <shape android:shape="oval">
            <stroke
                android:width="0.7dp"
                android:color="@color/md_theme_dark_shadow" />
            <solid android:color="@color/md_theme_dark_shadow" />
        </shape>
    </item>

</layer-list>